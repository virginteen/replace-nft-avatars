{
  "name": "Replace NFT Avatars",
  "description": "Replaces the image in the hexagonal avatars on Twitter",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "replace.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "nft-large.png",
    "nft-small.png"
  ]
}
